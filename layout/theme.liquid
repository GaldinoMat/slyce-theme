    <script>
      document.addEventListener('DOMContentLoaded', function() {
        var productSection = document.getElementById("ProductSection-{{ section_id }}-{{ product.id }}");
        var productTitle = productSection.querySelector("#productTitle");
        var variantDisplay = productSection.querySelector("#variantDisplay");
        var variantLabels = productSection.querySelectorAll("[data-variant-color-label]");

        variantLabels.forEach(function(label) {
          label.addEventListener('click', function(event) {
            var selectedVariant = label.innerText.trim();
            variantDisplay.innerText = selectedVariant; // Exibir o nome da variante na div
            productTitle.innerText = "{{ product.title }} - " + selectedVariant; // Atualizar o título do produto
          });
        });
      });
    </script>
    <script src="{{ 'splide.min.js' | asset_url }}" defer="defer"></script>
    <link rel="stylesheet" href="{{ 'splide.min.css' | asset_url }}">
    <script src="{{ 'init-splide.js' | asset_url }}" defer="defer"></script>
  </body>
</html>
